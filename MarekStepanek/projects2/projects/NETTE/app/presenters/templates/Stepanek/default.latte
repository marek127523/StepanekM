
{block content}

{$radek}
{$predek}<br>
{foreach $users as $item}
{$item}<br>
{/foreach}
{foreach $users as $item}  
    {$iterator->getCounter()}.{$item}<br>
{/foreach}
{foreach $users as $item}
   {$item}{sep},{/sep}
{/foreach}

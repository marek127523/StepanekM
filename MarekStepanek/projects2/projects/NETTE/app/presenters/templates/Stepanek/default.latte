            <td>{$i}-{$h}</td>
            {/for}
        <tr>
        {/for}
    
</table>
<table width="200" border="1">
    {foreach $users as $key =>$item}
        <tr>
            
            
            <td>
                {$key +1}
            </td>
            <td>
                {$item}
             </td>        
        <tr>
        {/foreach}
    
</table>
{/block}